<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="cn.sqkj.nsyl.eventsManager.pojo.NsEventsinfo" table="ns_eventsinfo">
        <id name="id" type="java.lang.Long">
            <column name="id" />
            <generator class="native" />
        </id>
        <property name="name" type="java.lang.String">
            <column name="name" length="50" />
        </property>
        <property name="isuse" type="java.lang.String">
            <column name="isuse" />
        </property>
        <property name="memo" type="java.lang.String">
            <column name="memo" length="65535" />
        </property>
        <property name="minpicture" type="java.lang.String">
            <column name="minpicture" length="100" />
        </property>
        <property name="picture" type="java.lang.String">
            <column name="picture" length="100" />
        </property>
        <property name="startTime" type="java.sql.Timestamp">
            <column name="start_time" />
        </property>
         <property name="endTime" type="java.sql.Timestamp">
            <column name="end_time"  />
        </property>
        <property name="flag" type="java.lang.String">
            <column name="flag" length="100" />
        </property>
    </class>
</hibernate-mapping>
