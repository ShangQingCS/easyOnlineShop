<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="cn.sqkj.nsyl.db.po.NsDictionaries" table="ns_dictionaries">
        <id name="id" type="java.lang.Long">
            <column name="id" />
            <generator class="native" />
        </id>
        <property name="type" type="java.lang.String">
            <column name="type" length="50" />
        </property>
        <property name="code" type="java.lang.String">
            <column name="code" length="50" />
        </property>
        <property name="name" type="java.lang.String">
            <column name="name" length="100" />
        </property>
        <property name="parentcode" type="java.lang.Long">
            <column name="parentcode" />
        </property>
        <property name="remark" type="java.lang.Long">
            <column name="remark" />
        </property>
        <property name="isuse" type="java.lang.String">
            <column name="isuse" length="1" />
        </property>
        <property name="isedit" type="java.lang.String">
            <column name="isedit" length="1" />
        </property>
        <property name="order" type="java.lang.Integer">
            <column name="order" length="11" />
        </property>
    </class>
</hibernate-mapping>
