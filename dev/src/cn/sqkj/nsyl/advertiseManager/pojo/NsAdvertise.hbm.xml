<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="cn.sqkj.nsyl.advertiseManager.pojo.NsAdvertise" table="ns_advertise">
        <id name="id" type="java.lang.Long">
            <column name="id" />
            <generator class="native" />
        </id>
        <property name="name" type="java.lang.String">
            <column name="name" length="100" />
        </property>
        <property name="memo" type="java.lang.String">
            <column name="memo" length="65535" />
        </property>
        <property name="imgurl" type="java.lang.String">
            <column name="imgurl" length="45" />
        </property>
        <property name="linkkind" type="java.lang.Long">
            <column name="linkkind" />
        </property>
        <property name="imglink" type="java.lang.Long">
            <column name="imglink" />
        </property>
        <property name="ordernumb" type="java.lang.Integer">
            <column name="ordernumb" length="11" />
        </property>
        <property name="type" type="java.lang.String">
            <column name="type" length="45" />
        </property>
        <property name="isuse" type="java.lang.String">
            <column name="isuse" length="1" />
        </property>
        <property name="kind" type="java.lang.Long">
            <column name="kind" length="20" />
        </property>
        <property name="createTime" type="java.sql.Timestamp">
            <column name="create_time" />
        </property>
        <property name="updateTime" type="java.sql.Timestamp">
            <column name="update_time" />
        </property>
        <property name="flag" type="java.lang.String">
            <column name="flag" length="1" />
        </property>
    </class>
</hibernate-mapping>
