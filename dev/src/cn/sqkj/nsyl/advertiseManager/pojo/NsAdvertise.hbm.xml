<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="cn.sqkj.nsyl.advertiseManager.pojo.NsAdvertise" table="ns_advertise">
        <id name="id" type="java.lang.Long">
            <column name="id" />
            <generator class="native" />
        </id>
        <property name="name" type="java.lang.String">
            <column name="name" length="100" />
        </property>
        <property name="memo" type="java.lang.String">
            <column name="memo" length="65535" />
        </property>
        <property name="imgurl" type="java.lang.String">
            <column name="imgurl" length="45" />
        </property>
        <property name="linkkind" type="java.lang.Long">
            <column name="linkkind" />
        </property>
        <property name="imglink" type="java.lang.String">
            <column name="imglink" length="45" />
        </property>
    </class>
</hibernate-mapping>
